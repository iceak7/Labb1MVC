@model Customer

<div class="row">
    <div class="col-md-6 offset-md-3">
        <h2 class="text-center">@Model.Name</h2>
        <table class="table mt-3">
            <tbody>
                <tr>
                    <th>Telephone Number</th>
                    <td>@Model.PhoneNr</td>
                </tr>
                <tr>
                    <th>Address</th>
                    <td>@Model.Address</td>
                </tr>
                <tr>
                    <th>Zip Code</th>
                    <td>@Model.ZipCode</td>
                </tr>
                <tr>
                    <th>City</th>
                    <td>@Model.City</td>
                </tr>
            </tbody>
        </table>
    </div>

</div>
